(define (problem task)
(:domain turtlebot)
(:objects
    wp0 wp1 wp2 - waypoint
    printer0 printer1 printer2 - printer
    robot0 robot1 - robot
)
(:init
    (robot_at robot0 wp0)
    (nocarrying_papers robot0)
    (undocked robot0)
    (localised robot0)

    (robot_at robot1 wp2)
    (nocarrying_papers robot1)
    (undocked robot1)
    (localised robot1)

    (dock_at wp0)

    (delivery_destination wp0) (at 1100.004 (not (delivery_destination wp0)))
    (delivery_destination wp1) (at 1100.004 (not (delivery_destination wp1)))
    (delivery_destination wp2) (at 1100.004 (not (delivery_destination wp2)))

    (= (distance wp0 wp1) 22)
    (= (distance wp1 wp0) 22)
    (= (distance wp0 wp2) 16)
    (= (distance wp2 wp0) 16)
    (= (distance wp0 printer0) 0)
    (= (distance printer0 wp0) 0)
    (= (distance wp0 printer1) 5)
    (= (distance printer1 wp0) 5)
    (= (distance wp0 printer2) 13)
    (= (distance printer2 wp0) 13)
    (= (distance wp1 wp2) 6)
    (= (distance wp2 wp1) 6)
    (= (distance wp1 printer0) 22)
    (= (distance printer0 wp1) 22)
    (= (distance wp1 printer1) 17)
    (= (distance printer1 wp1) 17)
    (= (distance wp1 printer2) 13)
    (= (distance printer2 wp1) 13)
    (= (distance wp2 printer0) 16)
    (= (distance printer0 wp2) 16)
    (= (distance wp2 printer1) 11)
    (= (distance printer1 wp2) 11)
    (= (distance wp2 printer2) 17)
    (= (distance printer2 wp2) 17)
    (= (distance printer0 printer1) 5)
    (= (distance printer1 printer0) 5)
    (= (distance printer0 printer2) 13)
    (= (distance printer2 printer0) 13)
    (= (distance printer1 printer2) 10)
    (= (distance printer2 printer1) 10)
)
(:goal (and
    (papers_delivered wp0)
    (papers_delivered wp2)
    (papers_delivered wp1)
))
)
